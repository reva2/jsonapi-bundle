{
    "name": "reva2/jsonapi-bundle",
    "description": "JSON API support for your Symfony REST API",
    "type": "symfony-bundle",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Revenko",
            "email": "dedsemen@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/framework-bundle": "^7.0",
        "reva2/jsonapi": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Reva2\\JsonApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Reva2\\JsonApiBundle\\Tests\\": "test/"
        }
    }
}
